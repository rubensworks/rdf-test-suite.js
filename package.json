{
  "name": "rdf-test-suite",
  "version": "1.12.1",
  "description": "Executes the RDF and SPARQL test suites.",
  "keywords": [
    "sparql",
    "rdf",
    "linked data",
    "test suite",
    "w3c"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/rdf-test-suite.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/rdf-test-suite.js/issues"
  },
  "homepage": "https://github.com/rubensworks/rdf-test-suite.js#readme",
  "license": "MIT",
  "bin": {
    "rdf-test-suite": "bin/Runner.js"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.json",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/log-symbols": "^3.0.0",
    "@types/minimist": "^1.2.0",
    "@types/n3": "^1.1.4",
    "@types/sax": "^1.0.1",
    "coveralls": "^3.0.0",
    "jest": "^24.3.0",
    "jest-rdf": "^1.3.0",
    "manual-git-changelog": "^1.0.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.0",
    "streamify-array": "^1.0.1",
    "ts-jest": "^25.1.0",
    "tslint": "^6.0.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.0.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "validate": "npm ls",
    "prepare": "npm run build",
    "version": "manual-git-changelog onversion"
  },
  "dependencies": {
    "@rdfjs/data-model": "^1.1.1",
    "arrayify-stream": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-stable-stringify": "^1.0.1",
    "jsonld-streaming-parser": "^1.1.1",
    "log-symbols": "^3.0.0",
    "minimist": "^1.2.0",
    "n3": "^1.1.1",
    "rdf-isomorphic": "^1.1.0",
    "rdf-literal": "^1.0.0",
    "rdf-object": "^1.1.0",
    "rdf-quad": "^1.3.0",
    "rdf-string": "^1.3.1",
    "rdf-terms": "^1.4.0",
    "rdfxml-streaming-parser": "^1.1.0",
    "relative-to-absolute-iri": "^1.0.5",
    "sparqljson-parse": "^1.5.0",
    "sparqlxml-parse": "^1.2.1",
    "stream-to-string": "^1.1.0",
    "streamify-string": "^1.0.1"
  }
}
